<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.robben.bbs.mapper.ImageMapper">    <select id="getImageLocationByConfessionWallId"            resultType="java.lang.String">        select location from  image where confessionWallId=#{confessionWallId}    </select>    <insert id="saveConfessionWallImages" parameterType="java.util.ArrayList">        insert into image(confessionWallId,location) values        <foreach collection="list" item="item" index="index"                 separator=",">            (#{item.confessionWallId},#{item.location})        </foreach>    </insert>    <delete id="deleteImageByConfessionWallId">        delete from image where confessionWallId=#{confessionWallId}    </delete></mapper>