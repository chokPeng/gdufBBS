<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.robben.bbs.mapper.OrdinaryUserMapper">    <select id="queryByUsername" parameterType="java.lang.String"            resultType="com.robben.bbs.pojo.OrdinaryUser">        select * from user where userName=#{username}    </select>    <insert id="register" parameterType="com.robben.bbs.pojo.OrdinaryUser">        insert into user(username,password,role)        values(#{username},#{password},'user')    </insert><!--    <resultMap id="userMap" type="">--><!--        <id column="userId" property="userId"></id>--><!--        <result column="username" property="username"></result>--><!--        <result column="password" property="password"></result>--><!--        <result column="level" property="level"></result>--><!--        <result column="email" property="email"></result>--><!--        <result column="location" property="location"></result>--><!--        <result column="introduction" property="introduction"></result>--><!--        <result column="avatar" property="avatar"></result>--><!--        <result column="sex" property="sex"></result>--><!--        <collection column="authorities-+--><!--4" property=""--><!--    </resultMap>--><!--    <select id="selectUser" parameterMap="String" resultMap="userMap">--><!--        select u.*--><!--        from user as u and authority as a--><!--        <where>--><!--            username = #{username}--><!--        </where>--><!--    </select>--></mapper>