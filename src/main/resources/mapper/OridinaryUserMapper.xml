<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.robben.bbs.mapper.OrdinaryUserMapper">    <select id="queryByUsername" parameterType="java.lang.String"            resultType="com.robben.bbs.pojo.OrdinaryUser">        select * from oridinaryUser where userName=#{username}    </select>    <insert id="register" parameterType="com.robben.bbs.pojo.OrdinaryUser">        insert into oridinaryUser(username,password,role)        values(#{username},#{password},'user')    </insert></mapper>